<?xml version="1.0"?>
<extension key="bot.partial.payment" type="module">
  <file>payment</file>
  <downloadUrl>https://github.com/twomice/BOT-Partial-Payment-Extension/archive/master.zip</downloadUrl>
  <name>BOT Partial Payment Extension (Joinery fork)</name>
  <description>This extension provides a framework which can be used to process multiple partial payments for event registrations in CiviCRM; this extension is a fork of the original by Backoffice Thinking</description>
  <urls>
    <url desc="Main Page">https://github.com/twomice/BOT-Partial-Payment-Extension</url>
  </urls>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Original by BackOffice Thinking; forked by Allen Shaw / Joinery</author>
    <email>allen@joineryhq.com</email>
  </maintainer>
  <releaseDate>2018-08-29</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Payment</namespace>
    <format>24.09.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <mixins>
    <mixin>entity-types-php@2.0.0</mixin>
  </mixins>
</extension>
